include ../blog/blog
include ../header_mixin/header_mixin
mixin footer01
  section.home01-footer
    .footer-01.py-lg-7.py-5
      .container
        .row.footer-inner
          .col-lg-4.mb-5.mb-lg-0.pr-lg-4
            .widget-text.company-info.in-dark
              .footer-logo
                a(href="/")
                  img(src="images/logo-light.png", alt="logo")
              p.mb-4 Without finance, plans stay as plans. That’s why we exists, we here to help clients make the most of these opportunities. We take the time to gain a deep understanding of our clients' financial requirements to bringing a passion for client success.
              .info
                .item.mb-3
                  i.ts-icon.la.la-phone.pr-1.fs--18
                  a(href="tel:18604193333") 1860-419-3333
                .item.mb-3
                  i.ts-icon.la.la-envelope.pr-1.fs--18
                  a(href="mailto:contact@ananci.com") consultant@ananci.com
                .item
                  i.ts-icon.la.la-clock-o.pr-1.fs--18
                  span Mon - Fri: 8am - 6pm
          .col-lg-4.d-none.d-lg-block.pr-lg-4
            .widget-latest-news.in-dark
              h4.widget-title-01.in-dark Latest News
              ul
                li
                  +blog08('images/blog-02.jpg', 'What Is Thought Leadership?', 'OCT 18, 2019')
                li
                  +blog08('images/blog-08.jpg', 'Creating a Productive & Happy Work Environment', 'OCT 28, 2019')
                li
                  +blog08('images/blog-03.jpg', 'A Day In the Life of a Financial', 'OCT 30, 2019')
          .col-lg-4
            .widget-subcrible-01.in-dark
              h4.widget-title-01.in-dark Subscribe
              p Subscribe to our newsletter to receive latest news on our services.
              .mb-4.input-lg
                input.mb-3.ts-input--ghost.form-control-lg(type='text' placeholder="Email Address")
                button.ts-btn.btn-lg.btn-color1(type='button') Subscribe
              .d-flex.justify-content-between.align-items-center
                span Follow Us: 
                .ts-social-list-01.align-right.in-dark
                  +tsSocialList01
    .copyright-02
      .container
        .copyright-inner.justify-content-center.justify-content-md-between
          .copyright-left
            p Copyright © 2019 
              a(href="/")
                strong  .Ananci 
              | All Right Reserved
          .copyright-right.d-none.d-md-block
            .widget-footer-links.in-dark
              ul
                li
                  a(href="/") Terms of use 
                li
                  a(href="/") Privacy Policy
                li
                  a(href="/") Cookie Policy
  a#to-the-top
    i.ts-icon.la.la-angle-up
    
