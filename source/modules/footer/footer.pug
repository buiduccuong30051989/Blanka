mixin footer03
  .home-01-footer
    .ts-overlay
    .ts-footer-05
      .ts-footer-info
        .container
          .ts-footer-info-inner
            .row.d-md-flex.d-block
              .col.col-lg-4.mb-md-0.mb-30
                .ts-icon-box-info-04
                  .icon
                    i.ts-icon.la.la-phone
                  .content
                    h5 Drop Us A Call
                    a(href="tel:18604193333") 0944-76-4545

              .col.col-lg-4.mb-md-0.mb-30
                .ts-icon-box-info-04
                  .icon
                    i.ts-icon.la.la-envelope
