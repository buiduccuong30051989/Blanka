                  figure.item(tabindex="3")
                    a(href="https://picsum.photos/id/100/1200/1200", data-size="1200x1200")  
                      img(src='https://picsum.photos/id/100/300/300', alt='thumb')
                  figure.item(tabindex="4")
                    a(href="https://picsum.photos/id/1001/1200/1200", data-size="1200x1200")  
                      img(src='https://picsum.photos/id/1001/300/300', alt='thumb')
                  figure.item(tabindex="5")
