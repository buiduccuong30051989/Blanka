                      data-items="1"
                      data-items-md="1"
                      data-items-sm="1"
                      data-items-xs="1"
                      data-margin="0"
                      data-dots="false"
                      data-arrows="true"
                      data-autoplay="false"
                      data-loop="true")
                      .item
                        img.card-img(src=cases.data[1].cover, alt='cover')
                      .item
                        img.card-img(src=cases.data[2].cover, alt='cover')
                      .item
                        img.card-img(src=cases.data[3].cover, alt='cover')

