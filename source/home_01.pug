extends modules/layout/layout.pug
include modules/script/lib
include modules/header_mixin/header_mixin
include modules/team/team
include modules/blog/blog
include modules/portfolio/portfolio
include modules/services/services
include modules/testimonial/testimonial
include modules/footer/footer
include modules/control/control
append head
  title="Home Page - 01"
  link(href="css/extention/photoswipe.min.css" rel="stylesheet")
  link(href="css/main.min.css" rel="stylesheet")
append header
  +headerHome01

append content
  .home-01-slider
    img(src="https://seniorthemes.com/images/home-05-slider-04.jpg")
  
  .home-01-services.py-7
    .container
      .row
        .col-lg-12.mb-lg-5
          .ts-section-heading-01.align-center
            h2 Choose Dotted Template For Your Purpose
          p.font-italic.text-center Phasellus id lacus neque. Proin vestibulum leo at turpis rhoncus, sed lacinia sem mollis. Etiam cursus bibendum dictum. Interdum et malesuada fames ac ante ipsum primis in faucibus. Etiam auctor eu nunc nec facilisis. Duis in felis eget eros tristique ornare. Maecenas volutpat, nisi id pretium imperdiet, odio neque pellentesque erat.
      .row-g-40
        - for(var i = 0; i < 3; i++) {
          .col-lg-4.mb-40
            +services09('la la-chart-pie', 'Consultant Services', 'We here to help', 'Donec lacinia mi rutrum sagittis consequat. Donec sagittis commodo.')
        - }

        - for(var i = 0; i < 3; i++) {
          .col-lg-4
            +services09('la la-hand-holding-usd', 'Consultant Services', 'We here to help', 'Donec lacinia mi rutrum sagittis consequat. Donec sagittis commodo.')
        - }

  .home-01-about-us.bg--whitesmoke.py-7
    .container
      .row
        .col-lg-7.d-flex.align-items-center
          img.thumb(src='http://templates.thememodern.com/dotted/images/Feature/1.png', alt='cover')
        .col-lg-5.pl-lg-5  
          .ts-section-heading-01
            h2 Why You Should Choose Us?
          p.mb-lg-4 Integer est arcu, congue nec vulputate faucibus, aliquam eget tellus. Aliquam laoreet aliquam mi, vitae tempus orci mattis vitae. Cras ultricies mauris est, ultricies sagittis diam euismod et.
          ul.list-style-01.list-square.color.mb-lg-4
            li FULLY RESPONSIVE 
            li TEMPLATE MULTIPLE 
            li PORTFOLIO COLUMNS 
            li AMAZING SLIDER 
            li 10 AMAZING SKINS 
            li AND MANY MORE...
          a(href="about.html").ts-btn.btn-color1 EXPLORE US
            i.ts-icon.la.la-plus.pl-3
        
  .home-01-projects.py-7.bg--white
    .container
      .row
        .col-lg-12
          .ts-section-heading-01.has-controls
            h2 Our Latest Projects
            .group-carousel-controls
              button.btn-carousel-01.case-prev
                i.ts-icon.las.la-angle-left
              button.btn-carousel-01.case-next
                i.ts-icon.las.la-angle-right

      .col-lg-12    
        .row
          .carousel.align-center-items(
            data-items="4" 
            data-items-md="3" 
            data-items-sm="1" 
            data-items-xs="1" 
            data-margin="0"
            data-center="false"
            data-dots="false"
            data-arrows="false" 
            data-autoplay='false' 
            data-hoverpause="true"
            data-autoplay-timeout="9000"
            data-loop="true"
            data-next="case-next"
            data-prev="case-prev")
            - for(var i = 0; i < 7; i++) {
              .item
                +portfolio04('https://picsum.photos/600/400?random=' + i, 'Project Card 06', 'Consultant', 'Commodo magna nulla nulla cillum sunt enim pariatur.')
            - }

  .home-01-testimonial
    .waveWrapper.waveAnimation#js-fade-elements
      .waveWrapperInner.bgTop
        .wave.waveTop
      .waveWrapperInner.bgMiddle
        .wave.waveMiddle
      .waveWrapperInner.bgBottom
        .wave.waveBottom
    
    .container
      .row
        .col-lg-12
          .ts-section-heading-01.align-center.in-dark
            h2 Testimonials
      .row
        .col-lg-12
          .carousel.align-center-items.arrows-intro.arrows-in-dark.carousel-focus(
            data-items="3" 
            data-items-md="3" 
            data-items-sm="1" 
            data-items-xs="1" 
            data-margin="40"
            data-center="true"
            data-dots="false"
            data-arrows="true" 
            data-autoplay='false' 
            data-hoverpause="true"
            data-autoplay-timeout="9000"
            data-loop="true")
            - for(var i = 1; i < 7; i ++) {
              .item
                +testimonial08('https://i.pravatar.cc/120?img=' + i, 'Adam Slander', 'Developer', 'Lorem ipsum dolor sit amet, consectetur adipiscing elit. Hic nihil fuit, quod quaereremus. Haec bene dicuntur, nec ego repugno, sed inter sese ipsa pugnant. Duo Reges: constructio interrete.')
            - }

  .home-01-blogs.bg--white
    .container
      .row
        .col-lg-12
          .ts-section-heading-01.has-controls
            h2 Our Latest Posts
            .group-carousel-controls
              button.btn-carousel-01.blog-prev
                i.ts-icon.las.la-angle-left
              button.btn-carousel-01.blog-next
                i.ts-icon.las.la-angle-right
      .row
        .col-lg-12
          .carousel.arrows-square.arrows-dark(
            data-items="3" 
            data-items-md="2" 
            data-items-sm="1" 
            data-items-xs="1" 
            data-margin="40"
            data-dots="false"
            data-arrows="false" 
            data-autoplay="true" 
            data-autoplay-timeout="9000" 
            data-loop="true"
            data-next="blog-next"
            data-prev="blog-prev")
            - for(var i = 1; i < 8; i++) {
              .item
                +blog10('Blog Title', 'Consultant', 'https://picsum.photos/600/300?random=' + i, 'Admin', 'Commodo magna nulla nulla cillum sunt enim pariatur. Aliquip velit minim laboris ipsum fugiat do qui sit ullamco cillum eu aute nulla.')
            - }

  .home-04-teams.bg--whitesmoke
    .ts-overlay
    .container
      .row
        .col-lg-12
          .ts-section-heading-01.align-center
            h2 Our Teams
      
      .carousel.align-center-items.arrows-intro.dots-theme.carousel-focus(
        data-items="3" 
        data-items-md="3" 
        data-items-sm="1" 
        data-items-xs="1" 
        data-margin="40"
        data-center="true"
        data-dots="false"
        data-arrows="true" 
        data-autoplay='false' 
        data-hoverpause="true"
        data-autoplay-timeout="9000"
        data-loop="true")
        - for(var i = 1; i < 8; i ++) {
          .item
            +team07('https://i.pravatar.cc/600?img=' + i, 'Adam Larson', 'Financial Consultant', 'Ullamco minim in enim consectetur labore voluptate et non non. Officia dolor qui cupidatat tempor qui ex labore veniam dolore.')
        - }

  +partner

  .pb-7.bg--whitesmoke
    .home-01-cta.py-7
      +boxCta01
  
  +footer03
  
  +control
