              tr
                th.product-remove  
                th.product-thumbnail  
                th.product-name Product
                th.product-price Price
                th.product-quantity Quantity
                th.product-subtotal Total
            tbody
              tr
                td.product-remove
                  a.remove(href="#") ×
                td.product-thumbnail
                  a(href="#")
                    img(src="https://picsum.photos/150/100?random=10", alt='demo')
                td.product-name(data-title="Product")
                  a(href="#") Bed Dream
                td.product-price(data-title="Price")
                  span.amount £2,189
                td.product-quantity(data-title="Quantity")
                  .quantity
                    input(type="number", value="1")
                td.product-subtotal(data-title="Total")
                  span.amount £2,189
              tr
                td.product-remove
